    <ProjectGuid>{7394B294-DE36-4E7B-B737-D3607D36E662}</ProjectGuid>
    <RootNamespace>CppConsoleDesktop.GenAI</RootNamespace>
    <WindowsTargetPlatformVersion>10.0</WindowsTargetPlatformVersion>
    <WindowsPackageType>None</WindowsPackageType>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.WindowsAppSDK" />
    <PackageReference Include="Microsoft.Windows.CppWinRT" />
    <PackageReference Include="Microsoft.ML.OnnxRuntimeGenAI.WinML" />
    <PackageReference Include="Microsoft.Windows.ImplementationLibrary" />
  </ItemGroup>

