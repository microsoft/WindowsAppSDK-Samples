<?xml version="1.0" encoding="utf-8"?>
<!-- This file was added for building in the CI pipeline. It is not used when building locally. -->
<configuration>
    <packageRestore>
        <!-- Allow NuGet to download missing packages -->
        <add key="enabled" value="True" />
        <!-- Automatically check for missing packages during build in Visual Studio -->
        <add key="automatic" value="True" />
    </packageRestore>
    <packageSources>
        <clear />
        <!-- publicly accessible NuGet feed -->
        <!-- upstream public NuGet.org - temporary fallback until the samples feed is updated -->
        <!-- a local directory to allow testing nupkg files without pushing to a remote feed -->
        <!-- This is also used by WinAppSDK's pipelines to validate a nupkg from a build -->
        <add key="localpackages" value="localpackages" />
        <add key="InternalAppSDK" value="https://microsoft.pkgs.visualstudio.com/ProjectReunion/_packaging/WinAppSDK-Developer-Builds/nuget/v3/index.json" />
        <add key="internal" value="https://pkgs.dev.azure.com/microsoft/ProjectReunion/_packaging/Project.Reunion.nuget.internal/nuget/v3/index.json" />
        <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    </packageSources>
    <disabledPackageSources />
</configuration>
